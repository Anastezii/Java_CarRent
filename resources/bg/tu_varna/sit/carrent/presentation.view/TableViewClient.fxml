<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<HBox xmlns="http://javafx.com/javafx/14" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bg.tu_varna.sit.carrent.presentation.controlers.TableClientConroller">
   <children>
      <AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #b0c4de;">
         <children>
            <Text layoutX="76.0" layoutY="49.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Client Table">
               <font>
                  <Font name="Berlin Sans FB" size="18.0" />
               </font>
            </Text>
            <TableView fx:id="TableClient" layoutX="96.0" layoutY="79.0" prefHeight="152.0" prefWidth="357.0">
              <columns>
                <TableColumn fx:id="idCl" prefWidth="75.0" text="id Client" />
                <TableColumn fx:id="clName" prefWidth="97.0" text="Client Name" />
                  <TableColumn fx:id="clPass" prefWidth="105.0" text="Client Password" />
                  <TableColumn fx:id="opId" prefWidth="79.0" text="operId" />
              </columns>
            </TableView>
            <Button fx:id="ReturnButton" layoutX="124.0" layoutY="250.0" mnemonicParsing="false" onAction="#handle" prefHeight="66.0" prefWidth="301.0" style="-fx-background-color: #b0c4de;" text="Return to Operator Window">
               <font>
                  <Font name="Berlin Sans FB" size="14.0" />
               </font>
            </Button>
            <ImageView fitHeight="46.0" fitWidth="67.0" layoutX="14.0" layoutY="20.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../img/tablet.png" />
               </image>
            </ImageView>
            <ImageView fitHeight="27.0" fitWidth="24.0" layoutX="366.0" layoutY="271.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../img/return.png" />
               </image>
            </ImageView>

         </children>
      </AnchorPane>
   </children>
</HBox>
