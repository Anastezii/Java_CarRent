<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="643.0" style="-fx-background-color: #b0c4de;"
            xmlns="http://javafx.com/javafx/14" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bg.tu_varna.sit.carrent.presentation.controlers.TableViewCarsController">
   <children>
      <Text layoutX="66.0" layoutY="35.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Cars table">
         <font>
            <Font name="Berlin Sans FB" size="18.0" />
         </font>
      </Text>
      <TableView fx:id="TableCars" layoutX="6.0" layoutY="60.0" prefHeight="200.0" prefWidth="634.0">
        <columns>
          <TableColumn fx:id="idCar" prefWidth="75.0" text="carId" />
          <TableColumn fx:id="model" prefWidth="75.0" text="Car Model" />
            <TableColumn fx:id="colour" prefWidth="75.0" text="Car Colour" />
            <TableColumn fx:id="year" prefWidth="75.0" text="Car Year" />
            <TableColumn fx:id="classCar" prefWidth="75.0" text="Car Class" />
            <TableColumn fx:id="smoker" prefWidth="106.0" text="Car Smoker Type" />
            <TableColumn fx:id="regNum" prefWidth="84.0" text="Car Reg Num" />
            <TableColumn fx:id="brand" prefWidth="84.0" text="Car Brand" />
        </columns>
      </TableView>
      <Button fx:id="BackButton" layoutX="180.0" layoutY="294.0" mnemonicParsing="false" onAction="#handle" prefHeight="53.0" prefWidth="326.0" style="-fx-background-color: #b0c4de;" text="Return to Admin Window">
         <font>
            <Font name="Berlin Sans FB" size="14.0" />
         </font>
      </Button>
      <ImageView fitHeight="46.0" fitWidth="39.0" layoutX="14.0" layoutY="10.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../img/tablet.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="31.0" fitWidth="39.0" layoutX="429.0" layoutY="305.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../img/return.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
